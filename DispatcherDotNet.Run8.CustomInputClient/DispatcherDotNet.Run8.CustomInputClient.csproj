<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Run8 Custom Input Client</Title>
    <Authors>Jonah Arndt, DispatcherDotNet</Authors>
    <Description>A wrapper client used to send messages to Run8 Train Simulator V3's custom input system.</Description>
    <PackageProjectUrl>https://www.dispatcherdotnet.com</PackageProjectUrl>
    <IncludeSymbols>False</IncludeSymbols>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <LangVersion>11</LangVersion>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Collections.Immutable" Version="8.0.0" />
  </ItemGroup>

</Project>